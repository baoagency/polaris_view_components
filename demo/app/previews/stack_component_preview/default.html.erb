<%= polaris_stack do |stack| %>
  <% stack.with_item do %>
    <%= polaris_badge { "Paid" } %>
  <% end %>
  <% stack.with_item do %>
    <%= polaris_badge { "Processing" } %>
  <% end %>
  <% stack.with_item do %>
    <%= polaris_badge { "Fulfilled" } %>
  <% end %>
  <% stack.with_item do %>
    <%= polaris_badge { "Completed" } %>
  <% end %>
<% end %>
