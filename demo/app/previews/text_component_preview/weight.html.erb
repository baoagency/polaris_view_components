<%= polaris_stack(vertical: true) do |stack| %>
  <% stack.with_item do %>
    <%= polaris_text(variant: :bodyMd, as: :p, font_weight: :bold) do %>
      Sales this year
    <% end %>
  <% end %>

  <% stack.with_item do %>
    <%= polaris_text(variant: :bodyMd, as: :p, font_weight: :semibold) do %>
      Sales this year
    <% end %>
  <% end %>

  <% stack.with_item do %>
    <%= polaris_text(variant: :bodyMd, as: :p, font_weight: :medium) do %>
      Sales this year
    <% end %>
  <% end %>

  <% stack.with_item do %>
    <%= polaris_text(variant: :bodyMd, as: :p, font_weight: :regular) do %>
      Sales this year
    <% end %>
  <% end %>
<% end %>
